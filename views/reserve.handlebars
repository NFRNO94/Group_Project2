<div id="reserve">
    <div class="container vertical-center fade-in-fwd">

        <div class="row" style="width: 100%;">
            <div class="col-12">

                <form class="reserve-table">
                    <div class="form-group mb-5 text-center">
                        <label for="name" style="color: orangered; font-size: 30px;">Select Your Hookah Flavor:</label>
                        <select class="mb-4" id="flavor_list" style="width: 100%;">
                        </select>
                    </div>

                    <div class="form-group mb-5 text-center">
                        <label for="name" style="color: orangered; font-size: 30px;">Your Name:</label>
                        <input type="text" class="form-control" id="name" aria-describedby="number-in-party"
                            placeholder="John Doe">
                    </div>

                    <div class="form-group mb-5 text-center">
                        <label for="email" style="color: orangered; font-size: 30px;">Your Email Address:</label>
                        <input type="email" class="form-control" id="email" aria-describedby="email address"
                            placeholder="john.doe@mail.com">
                    </div>

                    <div class="form-group mb-5 text-center">
                        <label for="phone" style="color: orangered; font-size: 30px;">Your Phone Number:</label>
                        <input type="tel" class="form-control" id="phone" aria-describedby="phone number"
                            placeholder="480-555-5555">
                    </div>

                    <button id="reserveTable" type="submit" class="btn btn-dark mt-3">Reserve A Table</button>
                    <a href="/" class="float-right mt-3">‚Üê Back To Home</a>
                </form>

                
            </div>
        </div>
       
                
        
    </div>
</div>

